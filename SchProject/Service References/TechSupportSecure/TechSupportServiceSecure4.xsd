      <xs:element minOccurs="0" name="Username" nillable="true" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="WorkerData" nillable="true" type="tns:WorkerData" />
  <xs:simpleType name="Bank">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MagyarNemzetiBank" />
      <xs:enumeration value="Erste" />
      <xs:enumeration value="OrszágosTakarékPénztár" />
      <xs:enumeration value="OTP" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Bank" nillable="true" type="tns:Bank" />
  <xs:simpleType name="Status">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Away" />
      <xs:enumeration value="Working" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Status" nillable="true" type="tns:Status" />
  <xs:complexType name="ArrayOfCustomerLoginData">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="CustomerLoginData" nillable="true" type="tns:CustomerLoginData" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfCustomerLoginData" nillable="true" type="tns:ArrayOfCustomerLoginData" />
